first = input("enter your first number:")
operator = input("enter operator (+,-,*,/,%):")
second = input("enter your last number:")

first=int(first)
second=int(second)

if operator == "+":
    print (first+second)
elif operator == "-":
    print (first-second)
elif operator == "/":
    print (first/second)
elif operator == "*":
    print (first*second)
elif operator == "%":
    print (first%second)
else:
    print ("invalid operation")
